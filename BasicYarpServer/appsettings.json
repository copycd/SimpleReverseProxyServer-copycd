{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "serviceJava": {
        "ClusterId": "serviceJavaCluster",
        "Match": {
          "Path": "/jv/{**catch-all}"
        },
        "Transforms": [
          { "PathPattern": "/{**catch-all}" }
        ]
      },
      "serviceCS": {
        "ClusterId": "serviceCSCluster",
        "Match": {
          "Path": "/cs/{**catch-all}"
        },
        "Transforms": [
          { "PathPattern": "/{**catch-all}" }
        ]
      },
      "serviceAll": {
        "ClusterId": "serviceAllCluster",
        "Match": {
          "Path": "/{**catch-all}"
        },
        "Transforms": [
          { "PathPattern": "/{**catch-all}" }
        ]
      }
    },
    "Clusters": {
      "serviceJavaCluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:25055/"
          }
        }
      },
      "serviceCSCluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:7073/"
          }
        }
      },
      "serviceAllCluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:25055/"
          }
        }
      }
    }
  }
}